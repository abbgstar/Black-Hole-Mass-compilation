	Journal = {\apj},
	Keywords = {quasars: supermassive black holes},
	Month = nov,
	Pages = {47},
	Title = {{Refining the M $_{BH}$-V $_{c}$ Scaling Relation with H I Rotation Curves of Water Megamaser Galaxies}},
